local MarketplaceService = game:GetService("MarketplaceService")
local GameName = MarketplaceService:GetProductInfo(game.PlaceId).Name
local Time = os.date("%H:%M:%S")
local PlayerLink = "["..game.Players.LocalPlayer.Name.."](https://www.roblox.com/users/"..game.Players.LocalPlayer.UserId.."/profile)"
local GameLink = "["..GameName.."](https://www.roblox.com/games/"..game.PlaceId..")"

local embed = {
	title = "Xtreme Hub Execution",
	description = "**Time:** "..Time.."\n\n**Player:** "..PlayerLink.."\n\n**Game:** "..GameLink.."\n\n**Executor:** "..getexecutorname().."\n\n**Screen Size:** "..tostring(ScreenSize).."\n\n**Screen Compatible:** "..(ScreenCompatible and "Yes" or "No"),
	color = 2171169
}

SendMessage(WebhookUrl, embed)
