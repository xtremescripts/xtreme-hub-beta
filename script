local function NewScriptButton(PlaceId, Script, Title)
	local Frame = Instance.new("Frame")
	local ImageLabel = Instance.new("ImageLabel")
	local TextLabel_3 = Instance.new("TextLabel")
	local TextButton_8 = Instance.new("TextButton")
	local TextLabel_4 = Instance.new("TextLabel")

	Frame.Parent = ScrollingFrame
	Frame.BackgroundColor3 = Color3.fromRGB(89, 89, 89)
	Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Frame.BorderSizePixel = 0
	Frame.Size = UDim2.new(0, 100, 0, 100)

	Frame:SetAttribute("Game", PlaceId)
	Frame:SetAttribute("Filtered", false)

	ImageLabel.Parent = Frame
	ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ImageLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
	ImageLabel.BorderSizePixel = 0
	ImageLabel.Position = UDim2.new(0.199, 0, 0.167, 0)
	ImageLabel.Size = UDim2.new(0, 72, 0, 72)
	local GameIcon = PlaceId ~= "Universal" and ("rbxassetid://"..MarketplaceService:GetProductInfo(PlaceId).IconImageAssetId) or "rbxassetid://12583118002"
	ImageLabel.Image = GameIcon

	TextLabel_3.Parent = Frame
	TextLabel_3.BackgroundTransparency = 1
	TextLabel_3.Position = UDim2.new(0.025, 0, 0.65, 0)
	TextLabel_3.Size = UDim2.new(0, 114, 0, 24)
	TextLabel_3.Font = Enum.Font.SourceSansSemibold
	local GameName = PlaceId ~= "Universal" and MarketplaceService:GetProductInfo(PlaceId).Name or "Universal"
	TextLabel_3.Text = GameName
	TextLabel_3.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_3.TextScaled = true

	TextLabel_4.Parent = Frame
	TextLabel_4.BackgroundTransparency = 1
	TextLabel_4.Position = UDim2.new(0.033, 0, 0, 0)
	TextLabel_4.Size = UDim2.new(0, 112, 0, 20)
	TextLabel_4.Font = Enum.Font.SourceSansBold
	TextLabel_4.Text = Title
	TextLabel_4.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_4.TextScaled = true
	TextLabel_4.Name = "TitleLabel"

	TextButton_8.Parent = Frame
	TextButton_8.BackgroundColor3 = (PlaceId == "Universal" or PlaceId == game.PlaceId) and Color3.fromRGB(0, 170, 255) or Color3.fromRGB(0, 211, 0)
	TextButton_8.BorderSizePixel = 0
	TextButton_8.Position = UDim2.new(0.05, 0, 0.832, 0)
	TextButton_8.Size = UDim2.new(0, 108, 0, 22)
	TextButton_8.AutoButtonColor = false
	TextButton_8.Font = (PlaceId == "Universal" or PlaceId == game.PlaceId) and Enum.Font.SourceSansBold or Enum.Font.SourceSans
	TextButton_8.Text = (PlaceId == "Universal" or PlaceId == game.PlaceId) and "</>" or "►" 
	TextButton_8.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextButton_8.TextSize = 22

	TextButton_8.MouseButton1Click:Connect(function()
		if game.PlaceId == PlaceId or PlaceId == "Universal" then
			loadstring(Script)()
		else
			local TeleportService = game:GetService("TeleportService")
			TeleportService:Teleport(PlaceId, game.Players.LocalPlayer)

		end
	end)
end

for _, Data in pairs(Scripts) do
	NewScriptButton(Data.PlaceId, Data.Script, Data.Title)
end

local SelectedMenuButton = nil

for _, MenuButton in pairs(Frame_2:GetChildren()) do
	if MenuButton:IsA("TextButton") then
		MenuButton.MouseButton1Click:Connect(function()
			SelectedMenuButton = MenuButton
			for _, MenuButton in pairs(Frame_2:GetChildren()) do
				if MenuButton:IsA("TextButton") then
					if MenuButton == SelectedMenuButton then
						MenuButton.TextTransparency = 0
						MenuButton.TextSize = 24
					else
						MenuButton.TextTransparency = 0.4
						MenuButton.TextSize = 20
					end
				end
			end
			game:GetService("TweenService"):Create(TextLabel, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {Position = MenuButton.Position}):Play()
		end)
	end
end

local function UpdateSearchResults()
	for _, ScriptFrame in pairs(ScrollingFrame:GetChildren()) do
		if ScriptFrame:IsA("Frame") and ScriptFrame:GetAttribute("Filtered") == false then
			ScriptFrame.Visible = (string.find(ScriptFrame.TitleLabel.Text:lower(), TextBox.Text:lower(), 1, true) ~= nil) or (string.find(ScriptFrame.TextLabel.Text:lower(), TextBox.Text:lower(), 1, true) ~= nil and ScriptFrame.TextLabel.Text ~= "Universal")
		end
	end
end

UpdateSearchResults()

for _, FilterButton in pairs(Frame:GetChildren()) do
	if FilterButton:IsA("TextButton") then
		FilterButton.MouseButton1Click:Connect(function()
			UpdateSearchResults()
			game:GetService("TweenService"):Create(TextLabel_5, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {Position = FilterButton.Position}):Play()
		end)
	end
end

TextButton_8.MouseButton1Click:Connect(function()
	for _, ScriptFrame in pairs(ScrollingFrame:GetChildren()) do
		if ScriptFrame:IsA("Frame") then
			ScriptFrame.Visible = true
			ScriptFrame:SetAttribute("Filtered", false)
		end
	end
end)

TextButton_9.MouseButton1Click:Connect(function()
	for _, ScriptFrame in pairs(ScrollingFrame:GetChildren()) do
		if ScriptFrame:IsA("Frame") and ScriptFrame:GetAttribute("Game") then
			ScriptFrame.Visible = game.PlaceId == ScriptFrame:GetAttribute("Game")
			ScriptFrame:SetAttribute("Filtered", game.PlaceId ~= ScriptFrame:GetAttribute("Game"))
		end
	end
end)

TextButton_10.MouseButton1Click:Connect(function()
	for _, ScriptFrame in pairs(ScrollingFrame:GetChildren()) do
		if ScriptFrame:IsA("Frame") and ScriptFrame:GetAttribute("Game") then
			ScriptFrame.Visible = (game.PlaceId ~= ScriptFrame:GetAttribute("Game") and ScriptFrame:GetAttribute("Game") ~= "Universal")
			ScriptFrame:SetAttribute("Filtered", not (game.PlaceId ~= ScriptFrame:GetAttribute("Game") and ScriptFrame:GetAttribute("Game") ~= "Universal"))
		end
	end
end)

TextButton_11.MouseButton1Click:Connect(function()
	for _, ScriptFrame in pairs(ScrollingFrame:GetChildren()) do
		if ScriptFrame:IsA("Frame") and ScriptFrame:GetAttribute("Game") then
			ScriptFrame.Visible = ScriptFrame:GetAttribute("Game") == "Universal"
			ScriptFrame:SetAttribute("Filtered", ScriptFrame:GetAttribute("Game") ~= "Universal")
		end
	end
end)

TextButton_6.MouseButton1Click:Connect(function()
	XH:Destroy()
end)

TextBox:GetPropertyChangedSignal("Text"):Connect(UpdateSearchResults)

local UserInputService = game:GetService("UserInputService")

local gui = Frame

local dragging
local dragInput
local dragStart
local startPos

local function update(input)
	local delta = input.Position - dragStart
	gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

gui.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = gui.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

gui.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)

local function CalculateScreenSize()
	local size = nil
	local gui = Instance.new("ScreenGui", game.Players.LocalPlayer.PlayerGui)
	local frame = Instance.new("Frame", gui)
	frame.Size = UDim2.new(1, 0, 1, 0)
	size = frame.AbsoluteSize
	gui:Destroy()
	if size then
		return size
	end
end

local ScreenSize = CalculateScreenSize()

local ScreenCompatible = (Frame.AbsoluteSize.X < ScreenSize.X and Frame.AbsoluteSize.Y < ScreenSize.Y) == true

function SendMessage(url, embed)
	local HttpService = game:GetService("HttpService")
	local headers = {
		["Content-Type"] = "application/json"
	}
	local data = {
		["embeds"] = {embed}
	}
	local body = HttpService:JSONEncode(data)
	local response = request({
		Url = url,
		Method = "POST",
		Headers = headers,
		Body = body
	})
	print("Sent")
end

